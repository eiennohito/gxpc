# make your prompt nice with GXP (like whatever[A/B/C]% )
precmd () {
  export PS1="%m:%c$(gxpc prompt 2> /dev/null)$ "
}
# set aliases for frequently used commands
alias mw='gxpc mw'
alias e='gxpc e'
alias smask='gxpc smask'
alias rmask='gxpc rmask'
alias savemask='gxpc savemask'
alias explore='gxpc explore'
alias use='gxpc use'
alias gcd='gxpc cd'
alias gexport='gxpc export'
